package org.xtext.sdu.iotvizualizerlanguage.generator

import java.util.List
import org.eclipse.xtext.generator.IFileSystemAccess2

class Postcompile {
	
	def postcompile( final IFileSystemAccess2 fsa, List<String> pageNames){
		
		compileUrls(pageNames)	
	}
	
	
	def compile
	
	def compileUrls(List<String> pageNames)
	'''
	from django.conf.urls import url
	
	from . import views
	
	urlpatterns = [
	    «FOR f:pageNames»
		«f.compile»
	    «ENDFOR»
	]
	'''
	
	def compile(String pageName)
	'''
	   url(r'^$', views.«pageName», name='«pageName»'),
	'''
}